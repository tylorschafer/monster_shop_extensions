<%= form_for @address do |f| %>
  <%= f.label :nickname %>
  <%= f.text_field :nickname, :placeholder => @address.nickname %>
  <%= f.label :street %>
  <%= f.text_field :street %>
  <%= f.label :city %>
  <%= f.text_field :city %>
  <%= f.label :state %>
  <%= f.text_field :state %>
  <%= f.label :zip %>
  <%= f.text_field :zip %>
  <%= f.submit %>
<% end %>
